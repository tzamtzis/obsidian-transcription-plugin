{
  "name": "obsidian-transcription-plugin",
  "version": "1.0.0",
  "description": "Transcribe audio files and extract actionable insights using AI - supporting Greek and English",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "transcription",
    "audio",
    "whisper",
    "ai",
    "meeting-notes",
    "greek",
    "multilingual"
  ],
  "author": "Panagiotis",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "obsidian": "latest",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "openai": "^4.20.0"
  }
}
